<?xml version="1.0" encoding="UTF-8"?>
<rulesets xmlns:ns1="http://castor.exolab.org/" ns1:defaultRuleSet="basic">
    <ns1:ruleset ns1:name="basic" ns1:language="JAVA">
        <ns1:description></ns1:description>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/JumbledIncrementer"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/ForLoopShouldBeWhileLoop"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/OverrideBothEqualsAndHashcode"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/DoubleCheckedLocking"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/ReturnFromFinallyBlock"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/UnconditionalIfStatement"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/BooleanInstantiation"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/CollapsibleIfStatements"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/ClassCastExceptionWithToArray"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/AvoidDecimalLiteralsInBigDecimalConstructor"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/MisplacedNullCheck"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/AvoidThreadGroup"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/BrokenNullCheck"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/BigIntegerInstantiation"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/AvoidUsingOctalValues"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/AvoidUsingHardCodedIP"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/CheckResultSet"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/AvoidMultipleUnaryOperators"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/ExtendsObject"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/CheckSkipResult"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/AvoidBranchingStatementAsLastInLoop"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/DontCallThreadRun"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/DontUseFloatTypeForLoopIndices"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptyCatchBlock"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptyIfStmt"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptyWhileStmt"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptyTryBlock"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptyFinallyBlock"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptySwitchStatements"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptySynchronizedBlock"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptyStatementNotInLoop"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptyInitializer"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptyStatementBlock"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/EmptyStaticInitializer"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/UnnecessaryConversionTemporary"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/UnnecessaryReturn"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/UnnecessaryFinalModifier"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/UselessOverridingMethod"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/UselessOperationOnImmutable"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/UnusedNullCheckInEquals"/>
        <ns1:rule ns1:ref="rulesets/java/basic.xml/UselessParentheses"/>
    </ns1:ruleset>
</rulesets>
