       for (int i=0; i<42;i++)
           foo();
     }
   
       ]]></example>
        <priority>4</priority>
        <properties>
            <property name="xpath">
                <value><![CDATA[
                     
    //ForStatement[not(Statement/Block)]
                     
                 ]]></value>
            </property>
