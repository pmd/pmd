        <properties>
            <property name="xpath">
                <value><![CDATA[
    
//Statement/StatementExpression/PrimaryExpression/PrimaryPrefix
[
./Name[
	@Image='notify'
	or ends-with(@Image,'.notify')
	]
or @Image='notify'
or (  ./AllocationExpression
    and ../PrimarySuffix[@Image='notify'] )
]
    
              ]]></value>
            </property>
        </properties>
    </rule>
