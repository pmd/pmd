    | "char"    {jjtThis.setKind(PrimitiveTypeKind.CHAR);}
    | "byte"    {jjtThis.setKind(PrimitiveTypeKind.BYTE);}
    | "short"   {jjtThis.setKind(PrimitiveTypeKind.SHORT);}
    | "int"     {jjtThis.setKind(PrimitiveTypeKind.INT);}
