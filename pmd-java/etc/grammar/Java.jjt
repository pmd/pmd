}

void DefaultValue():
{}
{
  "default" MemberValue()
}

void ModuleDeclaration():
{}
{
  AnnotationList()
  [LOOKAHEAD({isKeyword("open")}) <IDENTIFIER> {jjtThis.setOpen(true);}]
  LOOKAHEAD({isKeyword("module")}) <IDENTIFIER>
  ModuleName()
